public class Digimon {
    
    String name;
    String type;
    int level = 0;
    int attack = 0;
    int defense = 0;
    int intelligent = 0;
    int speed = 0;
    int experience = 0;
    int ability = 0;
    int health = 0;
    
    String name (String name){
        return name;
    }
    String type (String type){
        return type;
    }
    Integer level (int lvl){
        return this.level = lvl;
    }
    Integer health(int hp){
        return this.health = hp;
    }
    Integer attack(int atk){
        return this.attack = atk;
    }
    Integer defense(int def){
        return this.defense = def;
    }
    Integer intelligent(int INT){
        return this.intelligent = INT;
    }
    Integer speed(int spd){
        return this.speed = spd;
    }
    Integer ability(int abi){
        return this.ability = abi;
    }
    Integer experience(int exp){
        return this.experience = exp;
    }


}

public class Egg extends Digimon{
    int travel = 0;
    
    Integer travel (int travel){
        return this.travel = travel;
    }
    
    
}

public class driver {
    public static void main(String[] args){
        Digimon monster = new Digimon();
        Egg telur = new Egg();
        
        System.out.println("------------DIGIMON----------");
        System.out.println("Nama : " + monster.name("Omnimon"));
        System.out.println("Type : " + monster.type("Vaccine"));
        System.out.println("Level : " + monster.level(99));
        System.out.println("HP : " + monster.health(8302));
        System.out.println("Attack : " + monster.attack(2662));
        System.out.println("Defense : " + monster.defense(2236));
        System.out.println("Intelligent : " + monster.intelligent(2134));
        System.out.println("Speed : " + monster.speed(1882));
        System.out.println("Ability : " + monster.ability(1853));
        System.out.println("Experience : " + monster.experience(0));
        
        System.out.println("------------EGG-----------");
        System.out.println("Nama : " + monster.name("DigiEgg"));
        System.out.println("Type : " + monster.type("N/A"));
        System.out.println("Level : " + monster.level(0));
        System.out.println("HP : " + monster.health(0));
        System.out.println("Attack : " + monster.attack(0));
        System.out.println("Defense : " + monster.defense(0));
        System.out.println("Intelligent : " + monster.intelligent(0));
        System.out.println("Speed : " + monster.speed(0));
        System.out.println("Ability : " + monster.ability(0));
        System.out.println("Experience : " + monster.experience(0));
        System.out.println("Travel Distance : " + telur.travel(100));
    }
}
